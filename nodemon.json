{
    "restartable": "rs",
    "ignore": [".git", "node_modules/**/node_modules", "src/client", "**/*.tsx"],
    "verbose": true,
    "execMap": {
      "ts": "node --require ts-node/register/transpile-only --require tsconfig-paths/register"
    },
    "watch": ["src/server", "src/kibana", "typing"],
    "env": {
      "NODE_ENV": "development",
      "TS_NODE_PROJECT": "tsconfig.server.json"
    },
    "ext": "js,json,ts"
  }