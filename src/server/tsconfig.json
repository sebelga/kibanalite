{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "../",
    "module": "commonjs",
    "outDir": "./server/distServer",
    "lib": ["es2017"],
    "paths": {
      "src/core/*": ["./kibana/src/core/*"],
      "src/plugins/es_ui_shared/server": ["./kibana/src/plugins/es_ui_shared/server"],
      "src/plugins/es_ui_shared/common": ["./kibana/src/plugins/es_ui_shared/common"],
      "@kbn/i18n": ["./mocks/@kbn-i18n"],
      "@kbn/i18n/*": ["./mocks/@kbn-i18n/*"]
    }
  },
  "include": [
    "./kibana/**/*.ts",
    "./server/**/*.ts",
    "./typing",
  ],
  "exclude": [
    "./kibana/src/plugins/es_ui_shared/**/*.*"
  ]
}
